[color]
	ui = true
[alias]
    st = status
	co = checkout
	ci = commit -v
	lg = log --color --graph --pretty=format:'%Cred%h%Creset %C(bold blue)%an%Creset %Cgreen%cd%Creset - %s %C(yellow)%d%Creset' --abbrev-commit --
    gs = log --color --graph --stat
	stat = diff --stat
	df = difftool
	br = branch
	last = log -1 HEAD
