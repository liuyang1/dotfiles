[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = vimdiff
[core]
	editor = vim
	excludesfile = /home/liuy/.gitignore_global
	quotepath = false
[i18n]
	logoutputencoding = utf8
	commitencoding = utf8
[log]
	date = local
